{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "no-debugger": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": 0
  }
}
